<%= error_messages_for :dynamic_form %>
<% form_for @redirect_to_form do |form| %>

<table cellpadding="0" cellspacing="0" class="edit">
  <tr>
    <td class="label"><%= form.label :active %></td>
    <td class="value"><%= form.check_box :active %></td>
  </tr>
  <tr>
    <td class="label"><%= form.label :name %></td>
    <td class="value"><%= form.text_field :name %></td>
  </tr>
  <tr>
    <td class="label"><%= form.label :duplication_days, 'Duplication Timeframe' %></td>
    <td class="value"><%= form.text_field :duplication_days, :size => 4 %> days</td>
  </tr>
  <tr>
    <td class="label"><%= form.label :use_multistep %></td>
    <td class="value"><%= form.check_box :use_multistep %></td>
  </tr>
  <tr>
    <td colspan="2" class="submit"><%= submit_tag %></td>
  </tr>
</table>

<% end %>
