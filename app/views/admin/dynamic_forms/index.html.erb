<%= render_partial_if_exists "#{params[:action]}_content_for" %>

<table cellpadding="0" cellspacing="0" class="list">
  <tr>
    <th>Name</th>
    <th>Active</th>
    <th>Select</th>
  </tr>
<% @forms.each do |dynamic_form| -%>
  <tr class="<%= cycle('odd','even') %>">
    <td><%= dynamic_form.name %></td>
    <td align="center"><%= dynamic_form.active? ? "Yes" : "No" %></td>
    <td><%= link_to 'Select', admin_site_dynamic_form_url(@site, dynamic_form) %></td>
  </tr>
<% end -%>
</table>
