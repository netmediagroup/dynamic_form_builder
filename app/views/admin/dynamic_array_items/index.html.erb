<%= render_partial_if_exists "index_content_for" %>

<table cellpadding="0" cellspacing="0" class="list">
  <tr>
    <th>Display</th>
    <th>Value</th>
    <th>Select</th>
  </tr>
<% @array_items.each do |array_item| -%>
  <tr class="<%= cycle('odd','even') %>">
    <td><%= array_item.item_display %></td>
    <td><%= array_item.item_value %></td>
    <td align="center"><%= link_to 'Select', admin_dynamic_array_item_path(array_item) %></td>
  </tr>
<% end -%>
</table>
