<%= error_messages_for :array_item %>
<% form_for @form_action do |form| %>

<table cellpadding="0" cellspacing="0" class="edit">
  <tr>
    <td class="label"><%= form.label :item_display %></td>
    <td class="value"><%= form.text_field :item_display %></td>
  </tr>
  <tr>
    <td class="label"><%= form.label :item_value %></td>
    <td class="value"><%= form.text_field :item_value %></td>
  </tr>
  <tr>
    <td colspan="2" class="submit"><%= submit_tag %></td>
  </tr>
</table>

<% end %>
